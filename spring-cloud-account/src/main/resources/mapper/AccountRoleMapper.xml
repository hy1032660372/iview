<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hy.spring.cloud.account.mapper.AccountRoleMapper">

    <insert id="insertAccountRole" parameterType="com.hy.spring.cloud.account.domain.Entity.AccountRole">
        insert into  account_role
        (
            id,
            account_id,
            role_code
        )
        VALUES
        (
            #{id},
            #{accountId},
            #{roleCode}
        )
    </insert>

</mapper>